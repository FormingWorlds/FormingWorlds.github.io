# .gitignore for Jekyll websites

# Site output
_site/
.sass-cache/
.jekyll-cache/
.jekyll-metadata

# Dependencies
node_modules/
.bundle/
# vendor/
vendor/bundle/
vendor/cache/
vendor/gems/
vendor/ruby/

# Gem lock
Gemfile.lock

# Log files
*.log

# OS generated files
.DS_Store
Thumbs.db

# dotenv environment variables
.env

# Byebug command history
.byebug_history
